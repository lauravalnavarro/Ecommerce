);\n    }\n  };\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <CardElement options={CARD_ELEMENT_OPTIONS} />\n      {/* input ya preparado para ser validado que trae la biblioteca de Stripe */}\n      {/*  googlear stripe card test para acceder a las distintas tarjetas */}\n      <div\n        style={{\n          display: \"flex\",\n          justifyContent: \"space-between\",\n          marginTop: \"1rem\",\n        }}\n      >\n        <Button onClick={backStep} variant='outlined'>\n          Back\n        </Button>\n        <Button\n          type='submit'\n          disabled={!stripe}\n          variant='contained'\n          color='primary'\n        >\n          {loading ? (\n            <CircularProgress />\n          ) : (\n            `Pay ${accounting.formatMoney(getBasketTotal(basket), \"€\")}`\n          )}\n        </Button>\n      </div>\n    </form>\n  );\n};\n\nconst PaymentForm = ({ backStep, nextStep }) => {\n  return (\n    <>\n      <Review />\n      <Divider />\n      <Typography variant='h6' gutterBottom style={{ margin: \"20px 0\" }}>\n        Payment method\n      </Typography>\n      <Elements stripe={stripePromise}>\n        {/* permite acceder al objeto Stripe desde sus hijos */}\n        <CheckoutForm backStep={backStep} nextStep={nextStep} />\n        {/* Formulario de pago */}\n      </Elements>\n    </>\n  );\n};\n\nexport default PaymentForm;\n","/Users/lauravalerianavarroavellaneda/Documents/Github/Ecommerce/src/product-data.js",[],"/Users/lauravalerianavarroavellaneda/Documents/Github/Ecommerce/src/Pages/Confirmation.js",[],"/Users/lauravalerianavarroavellaneda/Documents/Github/Ecommerce/src/components/Product.js",["120"],"import { useState } from \"react\";\nimport clsx from \"clsx\";\nimport Card from \"@material-ui/core/Card\";\nimport CardHeader from \"@material-ui/core/CardHeader\";\nimport CardMedia from \"@material-ui/core/CardMedia\";\nimport CardContent from \"@material-ui/core/CardContent\";\nimport CardActions f